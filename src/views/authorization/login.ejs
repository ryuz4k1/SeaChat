<!DOCTYPE html>
<html lang="en" ng-app='seachat' ng-cloak>
<head>
    <meta charset="UTF-8" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>SeaChat</title>
    <link rel="stylesheet" type="text/css" href="../../../public/style/index.css">
</head>
<body>
    <div ng-controller='indexController'>
        <div class="modal">
            <div class="modal__inner">
                <div class="modal__header">
                  <h2>Welcome, please login.</h2>
                </div>
                <div class="modal__footer">
                    <a href="/auth/google"><i class="fa fa-google-plus" aria-hidden="true"></i>Login with Google+</a>
                </div>
            </div>
        </div>
    </div>
</body>
</html>

<!--AngularJs from Bower-->
<script src="../../bower_components/angular/angular.min.js"></script>

<!--MainJs from public/js-->
<script src="../../public/js/main.js"></script>

<!--Client side indexController-->
<script src="../../public/js/controllers/index-controller.js"></script>



